<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Loan Department">
	<listeners>
		<listener class-name="org.testpackage.Listeners"></listener>
	</listeners>
		<parameter name="URL" value="www.google.com"></parameter>
		<parameter name="UserName" value="111111"></parameter>
	<test name="Carloan">
    <classes>	
      <class name="org.testpackage.CarLoan">
		 <methods>
			<exclude name="mobile.*"></exclude> 
		 </methods>
		  </class>
    </classes>
  </test>
  <test name="HomeLoan">
    <classes>
      <class name="org.testpackage.Homeloan">
		  <methods>
			  <include name="apiloginHomeloan"></include>
		  </methods>
      </class>
    </classes>
  </test>
  <test name="Personal Loan">
	  	<parameter name="URL" value="Personalloan.com"></parameter>
    <classes>
      <class name="org.testpackage.TestNG1"/>
      <class name="org.testpackage.TestNG2"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
